extends ../../layout
block script
  -
    json = { success: false };

    if (patientTablet && typeof patientTablet === 'object' && patientTablet.id) {
      json = {
        success: true,
        data: {
          id: patientTablet.id,
          assignedAt: patientTablet.createdAt,
          assignedBy: patientTablet.assigner.fullName,
          assignedTablet: patientTablet.tablet.udid
        }
      }
    }